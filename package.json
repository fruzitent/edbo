{
  "dependencies": {
    "@mdx-js/loader": "^2.1.1",
    "@next/bundle-analyzer": "^12.1.5",
    "@next/mdx": "^12.1.4",
    "@shopify/react-compose": "^2.1.12",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.43",
    "@types/sharp": "^0",
    "cross-env": "^7.0.3",
    "next": "^12.1.4",
    "next-compose-plugins": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.49.11",
    "sharp": "^0.30.3",
    "typescript": "^4.6.2"
  },
  "description": "vstup.edbo.gov.ua parser",
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/prettier": "^2",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "typescript-coverage-report": "^0.6.4"
  },
  "license": "MIT",
  "name": "edbo",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "repository": {
    "type": "git",
