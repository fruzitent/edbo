    "mobx-state-tree": "^5.1.5",
    "next": "^12.1.6",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^11.0.0",
    "next-pwa": "^5.5.4",
    "next-themes": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "pg": "^8.7.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass": "^1.52.2",
    "sharp": "^0.30.6",
    "sql-template-strings": "^2.2.2",
    "typescript": "^4.7.3"
  },
  "description": "vstup.edbo.gov.ua parser",
  "devDependencies": {
    "@jest/globals": "^28.1.0",
    "@jest/types": "^28.1.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/eslint": "^8.4.2",
    "@types/jest": "^28.1.1",
    "@types/prettier": "^2.6.3",
    "@types/testing-library__jest-dom": "^5.14.3",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-environment-node": "^28.1.0",
    "jest-extended": "^2.0.0",
    "lint-staged": "^13.0.0",
    "next-page-tester": "^0.32.0",
    "prettier": "^2.6.2",
    "typescript-coverage-report": "^0.6.4"
  },
  "license": "MIT",
  "name": "edbo",
  "packageManager": "yarn@3.2.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fruzitent/edbo"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true run build",
    "build:debug": "run build --debug",
    "build:profile": "run build --profile",
    "ci": "run-s pretty build tsc:cov jest:cov",
    "dev": "cross-env NODE_OPTIONS='$NODE_OPTIONS --inspect' next dev",
    "docker": "docker compose -f docker-compose.yml up --build -d",
    "docker:prune": "docker system prune -f",
    "jest": "jest --expand --passWithNoTests --runInBand",
    "jest:cov": "run jest --ci --coverage",
    "jest:watch": "run jest --watch",
    "lint": "next lint --fix",
    "pre-commit": "lint-staged -v",
    "prepare": "husky install",
    "pretty": "prettier -u -w .",
    "ssl": "run-p ssl:req ssl:dh",
    "ssl:dh": "cd nginx && openssl dhparam -out dhparam.pem 4096",
    "ssl:rand": "openssl rand -base64 32",
    "ssl:req": "cd nginx && openssl req -x509 -nodes -days 365 -newkey rsa:4096 -keyout cert.key -out cert.crt",
    "start": "next start",
    "tsc": "tsc",
    "tsc:cov": "typescript-coverage-report --strict"
  }
}
